<% @allProfiles = Profile.ransack(params[:q]).result %>
 <div class="one">
  <p> <font size="6"> Results: </font></p>
   <% @allProfiles.each do |person| %>
<p> <font size="5"> 
<a href= <%= "/profiles/" + person.id.to_s %> >   <%= person.first_name %> <%= person.last_name %></a> 
<%= person.email %>
<%= person.landline %>
<%= person.cell %>
<%= person.nickname %>
<%= person.address %>
<%= person.spouse %>
<br/>
<% end %>
</div>